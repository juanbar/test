{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Rate</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fromdatetime</th>
                <td>{{ entity.fromDatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Todatetime</th>
                <td>{{ entity.toDatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ entity.rate }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('rate') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('rate_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
